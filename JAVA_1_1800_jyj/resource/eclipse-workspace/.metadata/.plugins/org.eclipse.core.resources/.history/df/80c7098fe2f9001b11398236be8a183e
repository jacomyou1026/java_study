package Sort;

public class insertEx {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i,j,tmp;
		int[] arr = {1,5,2,3,6,4};
		for(i=0;i<9;i++) {
			j=i;
			while(arr[j]>arr[j+1]) {
				tmp =arr[j];
				arr[j] = arr[j+1];
				arr[j+1] = tmp;
				j--;
			}
		}
		for(i=0;i<arr.length;i++) {
			System.out.print(arr[i]+", ");
		}
	}

}
