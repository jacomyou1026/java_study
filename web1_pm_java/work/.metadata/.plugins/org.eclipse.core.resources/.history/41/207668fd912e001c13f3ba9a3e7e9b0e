package Àç±Í;

import java.util.Scanner;

public class ÆÑÅä¸®¾ó {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num =sc.nextInt();
		fact(num);
		
	}
	static int fact(int num) {
		int result=0;
		while(num>=0) {
		result +=result*fact(num--);
		}
		return result;
	}
}
