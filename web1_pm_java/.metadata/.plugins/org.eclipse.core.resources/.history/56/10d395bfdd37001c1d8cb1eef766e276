package work;

import java.util.Random;
import java.util.Scanner;

public class updown {
		public String game(int result) {
			Scanner sc = new Scanner(System.in);
			int answer = sc.nextInt();
			int count = 1;
			String r = "";
			while(true) {
				if(answer>result) {
					count++;
					return "Down";
				}else if(answer<result){
					count++;
					return "up";
				}else if(answer==result){
					return count+"만에 정답";
				}
			}
		}

	}
